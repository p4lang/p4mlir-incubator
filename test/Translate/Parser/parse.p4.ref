match_kind {
    exact,
    ternary,
    lpm
}

struct empty {
}
parser p(in empty e, in int<10> sinit) {
    int<10> s = sinit;

    state start {
        s = (int<10>)(10s1);
        transition next;
    }
    state next {
        s = (int<10>)(10s2);
        transition accept;
    }
    state drop {
        transition reject;
    }
}
