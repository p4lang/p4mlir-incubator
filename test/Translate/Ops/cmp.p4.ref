match_kind {
    exact,
    ternary,
    lpm
}

action cmp() {
    bool res;
    bit<10> lhs = (bit<10>)(10w1);
    bit<10> rhs = (bit<10>)(10w2);

    res = (lhs == rhs);
    res = (lhs != rhs);
    res = (lhs < rhs);
    res = (lhs > rhs);
    res = (lhs <= rhs);
    res = (lhs >= rhs);
    return;
}

