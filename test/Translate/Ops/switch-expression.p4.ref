control c(inout bit<32> b) {
    apply {
        switch (b) {
            32w16: 32w32: {
                b = (bit<32>)(32w1);
            }
            32w64: {
                b = (bit<32>)(32w2);
            }
            default: {
                b = (bit<32>)(32w3);
            }
        }
    }
}
control ct(bit<32>);
package top(ct _c);
top(c()) main;

