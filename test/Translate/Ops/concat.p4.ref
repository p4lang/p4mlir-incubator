match_kind {
    exact,
    ternary,
    lpm
}

action bit5_concat_bit5(in bit<5> lhs, in bit<5> rhs, out bit<10> res) {
    res = (lhs ++ rhs);
    return;
}

action bit5_concat_bit10(in bit<5> lhs, in bit<10> rhs, out bit<15> res) {
    res = (lhs ++ rhs);
    return;
}

